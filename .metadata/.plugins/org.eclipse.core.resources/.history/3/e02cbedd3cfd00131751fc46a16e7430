import re
import os

skip_index = 0;
with open(os.path.join("benchmark","RBRtest2.txt"),'r') as RBRText:
    for x in RBRText:
        file_string = x.split(' ')[0]
        if re.match('^[0-9-]{3}+[A-Z]{1}+[a-z]{2,8}+[0-9-]{5}$', file_string):
            print('Success! Index %s' % skip_index)
            break
        x += 1     
    